{
  "homepage": "https://kirman442.github.io/accidents/",
  "name": "accidents",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "build": "vite build",
    "force-build": "npm run clean && vite build --emptyOutDir",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean": "rd /s /q \"node_modules\\.vite\" \"dist\" 2>nul || echo.",
    "hardclean": "taskkill /f /im node.exe & rd /s /q \"node_modules\" \"dist\" 2>nul || echo.",
    "reinstall": "npm run clean && npm install",
    "fullreset": "npm run hardclean && npm install"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.3",
    "@vis.gl/react-maplibre": "^8.0.1",
    "deck.gl": "^9.1.8",
    "maplibre-gl": "^5.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-select": "^5.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^15.15.0",
    "vite": "^6.2.0"
  }
}
